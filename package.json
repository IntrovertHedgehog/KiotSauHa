{
  "name": "KiotSauHa",
  "version": "0.1.0",
  "description": "Store Point of Sale is a desktop application developed by Hosting Domain and available for free download at www.storepointofsale.com.",
  "author": "Hosting Domain",
  "private": true,
  "main": "start.js",
  "build": {
    "appId": "kiot.sauha",
    "directories": {
      "buildResources": "assets"
    }
  },
  "dependencies": {
    "@electron/remote": "^2.1.3",
    "async": "^2.6.4",
    "bcrypt": "^6.0.0",
    "body-parser": "^1.18.2",
    "btoa": "^1.2.1",
    "dotenv": "^17.2.3",
    "electron-context-menu": "^0.15.1",
    "electron-squirrel-startup": "^1.0.1",
    "electron-store": "^5.1.0",
    "express": "^4.19.2",
    "express-fileupload": "^1.1.6",
    "html2canvas": "^1.0.0-rc.5",
    "http": "0.0.0",
    "https": "^1.0.0",
    "is-port-reachable": "^2.0.1",
    "jquery": "^3.4.1",
    "jsbarcode": "^3.11.0",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.3",
    "macaddress": "^0.2.9",
    "moment": "^2.29.4",
    "multer": "^1.4.2",
    "nedb": "^1.8.0",
    "patch-package": "^8.0.0",
    "path": "^0.12.7",
    "platform-folders": "^0.6.1",
    "postinstall-postinstall": "^2.1.0",
    "print-js": "^1.0.63",
    "socket.io": "^2.4.0",
    "sweetalert2": "^9.5.4",
    "upath": "^2.0.1",
    "xmlhttprequest": "^1.8.0"
  },
  "scripts": {
    "postinstall": "patch-package",
    "package-win": "electron-packager . POS --overwrite --asar=false --platform=win32 --arch=x64 --icon=assets/images/icon.ico --prune=true --out=release-builds --version-string.ProductName=\"POS\"",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.10.2",
    "@electron-forge/maker-deb": "^7.10.2",
    "@electron-forge/maker-rpm": "^7.10.2",
    "@electron-forge/maker-squirrel": "^7.10.2",
    "@electron-forge/maker-zip": "^7.10.2",
    "@electron-forge/plugin-auto-unpack-natives": "^7.10.2",
    "@electron-forge/plugin-fuses": "^7.10.2",
    "@electron/fuses": "^1.0.0",
    "cross-env": "^6.0.3",
    "electron": ">=22.3.25",
    "electron-is-dev": "^1.1.0",
    "nodemon": "^3.1.10"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "assets/icon.ico"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "platforms": [
            "win32"
          ]
        }
      ],
      "publishers": []
    }
  }
}
